<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecosd.project.carbonfootprint">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".MainActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity android:name=".AboutUsActivity"></activity>
        <activity android:name=".PrivacyPolicyActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".Alternatives"></activity>
        <activity android:name=".ChallangeActivity"></activity>


    </application>

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>
